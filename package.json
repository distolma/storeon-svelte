{
  "name": "@storeon/svelte",
  "version": "0.4.0",
  "description": "A tiny connector for Storeon and Svelte",
  "main": "index.js",
  "repository": "https://github.com/distolma/storeon-svelte.git",
  "author": "Dmytro Mostovyi <djmakaron2009@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "parcel serve example/index.html --open",
    "lint": "eslint ./*.js",
    "test": "jest --coverage && yarn lint && size-limit"
  },
  "devDependencies": {
    "@logux/eslint-config": "^29.0.1",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-es5": "^1.4.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-svelte": "^3.0.1",
    "size-limit": "^1.3.7",
    "svelte": "^3.6.7",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "storeon": "^0.9.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "size-limit": [
    {
      "path": "index.js",
      "limit": "281 B"
    }
  ],
  "eslintConfig": {
    "extends": "@logux/eslint-config/browser"
  }
}
